[core]
	excludesfile = ~/.gitignore_global
	quotepath = false
	pager = less
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	sw = switch
	hist = log --graph --pretty=format:\"%C(yellow)%h%C(auto) %s %C(yellow)(%C(cyan)%an, %C(magenta)%ad%C(yellow))%C(auto)%d\" --date=human
	type = cat-file -t
	dump = cat-file -p
	unstage = reset HEAD --
	last = log -1 HEAD
[pull]
	ff = only
