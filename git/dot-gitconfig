[core]
	excludesfile = ~/.gitignore_global
	quotepath = false
	pager = less
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	sw = switch
	hist = log --graph --oneline
	type = cat-file -t
	dump = cat-file -p
	unstage = reset HEAD --
	last = log -1 HEAD
[pull]
	ff = only
[include]
	path = .gitconfig.local
